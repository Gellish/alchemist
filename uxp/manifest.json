{
	"name": "Alchemist",
	"id": "cz.bereza.alchemist",
	"main": "index.html",
	"version": "0.24.0",
	"manifestVersion": 4,
	"host": {
		"app": "PS",
		"minVersion": "22.0.0"
	},
	"icons": [
		{
		    "width": 23, "height": 23, "path": "icons/dark.png", "scale": [ 1, 2 ],
		    "theme": [ "darkest", "dark", "medium" ]
		}, {
		    "width": 23, "height": 23, "path": "icons/light.png", "scale": [ 1, 2 ],
		    "theme": [ "lightest", "light" ]
		}
	],
	"entrypoints": [
		{
			"type": "panel",
			"id": "inspector",
			"label": {
				"default": "Alchemist"
			},
			"minimumSize": {
				"width": 230,
				"height": 200
			},
			"maximumSize": {
				"width": 2000,
				"height": 2000
			},
			"preferredDockedSize": {
				"width": 600,
				"height": 540
			},
			"preferredFloatingSize": {
				"width": 600,
				"height": 540
			}
		},
		{
			"type": "command",
			"id": "resetStateFn",
			"label": {
			    "default": "Reset Alchemist state"
			}
		}
	]
}